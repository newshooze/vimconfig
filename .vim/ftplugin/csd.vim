endif

command! Play execute "!csound %"
command! Wave execute "!csound -o test.wav %"
command! Templatef12 execute "source ~/.vim/template/csdf12.vim"

nmap <buffer> <F5> :Play<CR>
imap <buffer> <F5> <ESC>:Play<CR>

nmap <buffer> <S-F5> :Wave<CR>
imap <buffer> <S-F5> <ESC>:Wave<CR>

nmap <buffer> <F12> <ESC>:Templatef12<CR><ESC>gg

" Opens Help window for word at cursor
map <buffer> <S-K> <C-W><C-]>
" map <C-L> to line completion
set tags=~/.vim/doc/tags
set helpfile=/home/vinny/.vim/doc/csound.help.gz
set dict=/home/vinny/.vim/dict/csdmodes
set complete+=k
set hidden


